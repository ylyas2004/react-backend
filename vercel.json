{
    "version": 2,
    "framework": "express",
    "builds": [
      {
        "src": "./bin/www",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "./bin/www"
      }
    ],
    "scripts": {
      "build": "echo 'No build step required'",
      "start": "npm start"
    }
  }